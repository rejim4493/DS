#include <stdio.h>

int main()
{
    int x=0, tot=0, do100 = 0, do20 = 0, do10 = 0, do5 = 0, do1 = 0;
    scanf("%d", &x);
    do100 = x/100;//總額除以100後有幾個100
    do20 = (x-100*do100)/20;//總額減掉幾個100後除以20，有幾個20
    do10 = (x-100*do100-20*do20)/10;//總額減掉幾個100、減掉幾個20，除以10，有幾個10
    do5 = (x-100*do100-20*do20-10*do10)/5;
    do1 = (x-100*do100-20*do20-10*do10-5*do5);
    tot = do100 + do20 + do10 + do5 + do1;

    printf("%d", tot);

    return 0;
}
